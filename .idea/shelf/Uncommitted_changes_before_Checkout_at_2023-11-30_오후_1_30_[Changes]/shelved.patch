<+><!-- http://127.0.0.1:8000/community/team -->\r\n\r\n\r\n<link rel=\"stylesheet\" href=\"../../static/community/css/postDetailStyle.css\"/>\r\n<link rel=\"stylesheet\" href=\"../../static/community/css/teamlist.css\"/>\r\n\r\n{% extends 'community/base.html' %}\r\n\r\n{% block main_area %}\r\n\r\n    <div class=\"wrapper\">\r\n        <div class=\"content_wrapper\">\r\n            <div class=\"title\">\r\n                <h2>팀원 모집</h2>\r\n                <br/>\r\n            </div>\r\n            <div class=\"content_nav\">\r\n                <a href=\"#\">상세정보</a>\r\n                <a href=\"#\">팀원모집</a>\r\n                <button style=\"color: white; font-size: 14px\">스크랩</button>\r\n            </div>\r\n            <div class=\"content_box\">\r\n\r\n                <button id=\"createPostBtn\" onclick=\"window.location.href='{% url 'community:TeamPostForm' %}'\">팀원 모집 글쓰기</button>\r\n\r\n\r\n\r\n                {% if team_list.exists %}\r\n                    <!-- 추가된 부분: 표 형식으로 게시글 표시 -->\r\n                    <table>\r\n                        <tbody>\r\n                        {% for team in team_list %}\r\n                            <tr>\r\n                                <td>\r\n                                    <a href=\"{% url 'community:team_detail' team.id %}\">\r\n                                        <h2>{{ team.title }}</h2>\r\n                                        {#                        <p>{{ team.content }}</p>#}\r\n                                        <p>{{ team.time }}</p>\r\n                                </td>\r\n                            </tr>\r\n                            <tr>\r\n                                <td>\r\n                                    <hr class=\"post-separator\">\r\n                                </td>\r\n                            </tr>\r\n                        {% endfor %}\r\n                        </tbody>\r\n                    </table>\r\n                {% else %}\r\n                    <h3>아직 팀 게시글이 없습니다.</h3>\r\n                {% endif %}\r\n\r\n\r\n                <br/></div>\r\n        </div>\r\n    </div>\r\n    <br/>\r\n    </div>\r\n    </div>\r\n    </div>\r\n\r\n{% endblock %}\r\n
